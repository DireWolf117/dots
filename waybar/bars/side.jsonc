{
  "layer": "top",
  "position": "left",
  "width": 47,
  "spacing": 0,
  "modules-left": [
    "custom/time-of-day",
    "clock#time",
    "custom/separator1",
    "clock#date"
  ],
  "modules-center": [
    "hyprland/workspaces"
  ],
  "modules-right": [
    "network#icon",
    "network#percentage",
    "custom/separator2",
    "pulseaudio#percentage",
    "pulseaudio"
  ],
  "custom/time-of-day": {
    "exec": "~/.config/waybar/scripts/time-of-day.sh",
    "interval": 20,
    "tooltip": false
  },
  "clock#time": {
    "format": "{:%H\n%M}",
    "tooltip": false
  },
  "clock#date": {
    "format": "{:%d\n%m}",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
  },
  "hyprland/workspaces": {
    "format": "{icon}",
    "on-click": "activate",
    "format-icons": {
      "1": "‰∏Ä",
      "2": "‰∫å",
      "3": "‰∏â",
      "4": "Âõõ",
      "5": "‰∫î",
      "6": "ÂÖ≠",
      "7": "‰∏É",
      "8": "ÂÖ´",
      "9": "‰πù"
    },
    "all-outputs": true,
    "sort-by-number": true
  },
  "pulseaudio": {
    "format": "{icon}",
    "format-muted": "Û∞ñÅ",
    "format-icons": {
      "default": [
        "Û∞ïæ"
      ]
    },
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "on-scroll-up": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+",
    "on-scroll-down": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-",
    "tooltip": false
  },
  "pulseaudio#percentage": {
    "format": "{volume}",
    "format-muted": "-",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "on-scroll-up": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+",
    "on-scroll-down": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-",
    "tooltip": false
  },
  "network#icon": {
    "format": "{icon}",
    "format-wifi": "{icon}",
    "format-ethernet": "‚ñà",
    "format-disconnected": "‚úï",
    "format-icons": [
      "üñß"
    ],
    "tooltip-format": "{essid}",
    "tooltip-format-ethernet": "Ethernet Connected",
    "tooltip-format-disconnected": "Disconnected",
    "on-click": "foot --title=iwctl iwctl",
    "interval": 5
  },
  "network#percentage": {
    "format": "{signalStrength}",
    "format-wifi": "{signalStrength}",
    "format-ethernet": "ETH",
    "format-disconnected": "-",
    "tooltip-format": "{essid}",
    "tooltip-format-ethernet": "Ethernet Connected",
    "tooltip-format-disconnected": "Disconnected",
    "on-click": "foot --title=iwctl iwctl",
    "interval": 5
  },
  "custom/separator1": {
    "format": "‚îÄ‚îÄ‚îÄ",
    "tooltip": false
  },
  "custom/separator2": {
    "format": "‚îÄ‚îÄ‚îÄ",
    "tooltip": false
  }
}
